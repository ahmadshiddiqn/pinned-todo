{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "pinned-todo",
  "version": "0.1.0",
  "identifier": "com.baramulti.pinned-todo",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "ToDo List",
        "width": 380,
        "height": 600,
        "minHeight": 380,
        "minWidth": 380,
        "maxWidth": 380,
        "resizable": true,
        "fullscreen": false,
        "alwaysOnTop": false,
        "decorations": false,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
